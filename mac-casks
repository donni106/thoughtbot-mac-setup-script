fancy_echo "Installing cask applications"

# Browsers
brew install --cask \
  brave-browser \
  choosy \
  firefox \
  google-chrome \
  microsoft-edge \
  opera \
  sizzy

# Development
brew install --cask \
  android-studio \
  dbeaver-community \
  docker \
  flipper \
  fork \
  insomnia \
  mjml \
  sublime-text \
  zulu

# Others
